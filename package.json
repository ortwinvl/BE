{
  "name": "acsisrelationmanagement",
  "version": "1.0.0",
  "description": "Backend for the Acsis RM",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run prod",
    "prod": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "migdev": "npm run db:migrate && npm run dev",
    "db:up": "echo creating && npm run db:create && echo created && npm run db:migrate && echo migrated",
    "db:reset": "npm run db:drop && npm run db:up",
    "db:create": "sequelize db:create",
    "db:createproduction": "cross-env NODE_ENV=production sequelize db:create",
    "db:drop": "[[ $NODE_ENV == production ]] && exit 1 || sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrateproduction": "cross-env NODE_ENV=production sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:rollback:all": "sequelize db:migrate:undo:all",
    "eslint": "eslint ./",
    "eslint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "assert": "^2.1.0",
    "babel-loader": "^9.1.3",
    "browserify-zlib": "^0.2.0",
    "constants-browserify": "^1.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-vue": "^9.24.0",
    "https-browserify": "^1.0.0",
    "nodemon": "^3.0.3",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.4",
    "querystring-es3": "^0.2.1",
    "sequelize-cli": "^6.6.2",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "util": "^0.12.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@types/express-fileupload": "^1.4.4",
    "@xmldom/xmldom": "^0.8.10",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-session": "^1.18.0",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "sequelize": "^6.37.2",
    "tedious": "^18.1.0",
    "weeknumber": "^1.2.1",
    "xmlbuilder": "^15.1.1",
    "xpath": "^0.0.34"
  }
}
